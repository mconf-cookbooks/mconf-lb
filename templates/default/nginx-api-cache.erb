location = /bigbluebutton/api {
  proxy_cache api;
  proxy_cache_valid 200 60m;
  proxy_ignore_headers "Set-Cookie";
  proxy_hide_header "Set-Cookie";
  include "/etc/nginx/includes/mconf-lb-node.conf";
}
